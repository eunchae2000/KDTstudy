package javabasic;

public class Hi {

	public static void main(String[] args) {
		// 나의 첫 자바프로그램
		// 컴퓨터의 모니에 프린터해줘.
		
		//문자열(string)은 " "
		System.out.println("안녕하세요..반갑습니다.");
		//저장 ctrl + s(save)
		//실 ctrl + fll
		System.out.println("저는 이은채라고 합니다.");
		System.out.println("현재 대학교 4학년에 재학중입니다.");
		System.out.println("저는 23살 입니다.");
		System.out.println("자바를 배우고 있습니다.");
	}

}
